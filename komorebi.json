{
  "$schema": "https://raw.githubusercontent.com/LGUG2Z/komorebi/v0.1.24/schema.json",
  "active_window_border": true,
  "active_window_border_colours": {
    "single": "#86DEC2",
    "stack": "#00a542",
    "monocle": "#F73837"
  },
  "app_specific_configuration_path": "$Env:USERPROFILE/applications.yaml",
  "border_offset": -1,
  "border_width": 1,
  "cross_monitor_move_behaviour": "Insert",
  "default_container_padding": 8,
  "default_workspace_padding": 0,
  "float_rules": [
    [
      {
        "kind": "Exe",
        "id": "steamwebhelper.exe",
        "matching_strategy": "Equals"
      },
      {
        "kind": "Title",
        "id": "Menu",
        "matching_strategy": "Equals"
      }
    ],
    [
      {
        "kind": "Exe",
        "id": "steamwebhelper.exe",
        "matching_strategy": "Equals"
      },
      {
        "kind": "Title",
        "id": "Launching...",
        "matching_strategy": "Equals"
      }
    ],
    [
      {
        "kind": "Exe",
        "id": "Arc.exe",
        "matching_strategy": "Equals"
      },
      {
        "kind": "Title",
        "id": "Arc extension popup",
        "matching_strategy": "Equals"
      }
    ],
    [
      {
        "kind": "Exe",
        "id": "Arc.exe",
        "matching_strategy": "Equals"
      },
      {
        "kind": "Title",
        "id": "Arc picture in picture",
        "matching_strategy": "Equals"
      }
    ],
    {
      "kind": "Exe",
      "id": "Authy Desktop.exe",
      "matching_strategy": "Equals"
    },
    [
      {
        "kind": "Exe",
        "id": "Kindle.exe",
        "matching_strategy": "Equals"
      },
      {
        "kind": "Title",
        "id": "Sync to Most Recent Page Read",
        "matching_strategy": "Equals"
      }
    ],
    {
      "kind": "Exe",
      "id": "spacedeskServiceTray.exe",
      "matching_strategy": "Equals"
    }
  ],
  "monitors": [
    {
      "workspaces": [
        {
          "name": "I",
          "layout": "BSP"
        },
        {
          "name": "II",
          "layout": "VerticalStack"
        },
        {
          "name": "III",
          "layout": "HorizontalStack"
        },
        {
          "name": "IV",
          "layout": "UltrawideVerticalStack"
        },
        {
          "name": "V",
          "layout": "Rows"
        }
      ]
    }
  ],
  "window_hiding_behaviour": "Cloak"
}
